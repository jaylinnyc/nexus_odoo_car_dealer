<odoo>
    <template id="product_reserve_now" inherit_id="website_sale.cta_wrapper">
<xpath expr="//a[@id='add_to_cart']" position="replace">
            <a id="reserve_now_btn"
               role="button"
               t-attf-href="/reservation?product_id={{product.id}}"
               class="btn btn-primary btn-md flex-grow-1"
             >
                Reserve Now
            </a>
        </xpath>
    </template>
</odoo>